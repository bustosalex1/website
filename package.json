{
    "name": "website",
    "type": "module",
    "version": "0.3",
    "packageManager": "pnpm@9.4.0",
    "scripts": {
        "stamp": "echo \"COMMIT_HASH=$(git rev-parse --short HEAD)\"> .env && echo \"WEBSITE_VERSION=$(node -p \"require('./package.json').version\")\">>.env",
        "dev": "astro dev",
        "start": "astro dev",
        "build": "pnpm stamp && astro build",
        "preview": "astro preview",
        "astro": "astro",
        "format": "prettier . --write",
        "lint": "prettier --check . && eslint . && svelte-check --tsconfig ./tsconfig.json"
    },
    "dependencies": {
        "@astrojs/mdx": "^3.1.9",
        "@astrojs/svelte": "^5.7.3",
        "@astrojs/tailwind": "^5.1.2",
        "@astrojs/ts-plugin": "^1.10.4",
        "astro": "^4.16.16",
        "daisyui": "^4.12.14",
        "fuse.js": "^7.0.0",
        "katex": "^0.16.11",
        "rehype-katex": "^7.0.1",
        "remark-math": "^6.0.0",
        "svelte": "^4.2.19",
        "tailwindcss": "^3.4.15",
        "theme-change": "^2.5.0"
    },
    "devDependencies": {
        "@iconify-json/carbon": "^1.2.4",
        "@iconify-json/line-md": "^1.2.2",
        "@iconify-json/octicon": "^1.2.1",
        "@tailwindcss/typography": "^0.5.15",
        "@types/eslint": "^9.6.1",
        "autoprefixer": "^10.4.20",
        "eslint": "^9.15.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-svelte": "^2.46.0",
        "globals": "15.9.0",
        "postcss": "^8.4.49",
        "prettier": "^3.4.1",
        "prettier-plugin-astro": "^0.14.1",
        "prettier-plugin-svelte": "^3.3.2",
        "sharp": "^0.33.5",
        "svelte-check": "^3.8.6",
        "typescript": "^5.7.2",
        "typescript-eslint": "^8.16.0",
        "unplugin-icons": "^0.19.3",
        "vite": "^5.4.11"
    }
}
